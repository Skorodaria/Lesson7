package Lesson7;

import java.security.spec.RSAOtherPrimeInfo;

public class Main {
    public static void main(String[] args) {
        Cat[] cats = {
                new Cat("Мири", 900),
                new Cat("Рамзес", 50),

        };

        Plate plate = new Plate(43);

        for (Cat cat : cats) {
            cat.eat(plate);
            cat.info();
        }

        plate.addFood(800);
        plate.info();
    }

    }
